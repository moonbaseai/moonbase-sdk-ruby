module Moonbase
  module Models
    type inbox_conversation =
      {
        id: String,
        bulk: bool,
        created_at: Time,
        draft: bool,
        follow_up: bool,
        last_message_at: Time,
        spam: bool,
        state: Moonbase::Models::InboxConversation::state,
        subject: String,
        tags: ::Array[Moonbase::InboxConversation::Tag],
        trash: bool,
        type: :inbox_conversation,
        unread: bool,
        updated_at: Time,
        inbox: Moonbase::Inbox,
        messages: ::Array[Moonbase::EmailMessage],
        unsnooze_at: Time
      }

    class InboxConversation < Moonbase::Internal::Type::BaseModel
      attr_accessor id: String

      attr_accessor bulk: bool

      attr_accessor created_at: Time

      attr_accessor draft: bool

      attr_accessor follow_up: bool

      attr_accessor last_message_at: Time

      attr_accessor spam: bool

      attr_accessor state: Moonbase::Models::InboxConversation::state

      attr_accessor subject: String

      attr_accessor tags: ::Array[Moonbase::InboxConversation::Tag]

      attr_accessor trash: bool

      attr_accessor type: :inbox_conversation

      attr_accessor unread: bool

      attr_accessor updated_at: Time

      attr_reader inbox: Moonbase::Inbox?

      def inbox=: (Moonbase::Inbox) -> Moonbase::Inbox

      attr_reader messages: ::Array[Moonbase::EmailMessage]?

      def messages=: (
        ::Array[Moonbase::EmailMessage]
      ) -> ::Array[Moonbase::EmailMessage]

      attr_reader unsnooze_at: Time?

      def unsnooze_at=: (Time) -> Time

      def initialize: (
        id: String,
        bulk: bool,
        created_at: Time,
        draft: bool,
        follow_up: bool,
        last_message_at: Time,
        spam: bool,
        state: Moonbase::Models::InboxConversation::state,
        subject: String,
        tags: ::Array[Moonbase::InboxConversation::Tag],
        trash: bool,
        unread: bool,
        updated_at: Time,
        ?inbox: Moonbase::Inbox,
        ?messages: ::Array[Moonbase::EmailMessage],
        ?unsnooze_at: Time,
        ?type: :inbox_conversation
      ) -> void

      def to_hash: -> {
        id: String,
        bulk: bool,
        created_at: Time,
        draft: bool,
        follow_up: bool,
        last_message_at: Time,
        spam: bool,
        state: Moonbase::Models::InboxConversation::state,
        subject: String,
        tags: ::Array[Moonbase::InboxConversation::Tag],
        trash: bool,
        type: :inbox_conversation,
        unread: bool,
        updated_at: Time,
        inbox: Moonbase::Inbox,
        messages: ::Array[Moonbase::EmailMessage],
        unsnooze_at: Time
      }

      type state = :unassigned | :active | :closed | :waiting

      module State
        extend Moonbase::Internal::Type::Enum

        UNASSIGNED: :unassigned
        ACTIVE: :active
        CLOSED: :closed
        WAITING: :waiting

        def self?.values: -> ::Array[Moonbase::Models::InboxConversation::state]
      end

      type tag = { id: String, name: String, type: :tag }

      class Tag < Moonbase::Internal::Type::BaseModel
        attr_accessor id: String

        attr_accessor name: String

        attr_accessor type: :tag

        def initialize: (id: String, name: String, ?type: :tag) -> void

        def to_hash: -> { id: String, name: String, type: :tag }
      end
    end
  end
end
